<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<launch>
	<arg name="freq" default="100"/>⏎
	<arg name="heading_degrees" default="-90"/>⏎
	<!-- we need to use radians, but they are not very easy to remember, anyway, you can use both like this -->⏎
	<arg name="heading_rad" default="$(eval arg('heading_degrees')/180*3.14159265)"/>⏎

	<node pkg="tf" type="static_transform_publisher" name="subject_heading" args="0 0.1 0  0 0 0 map imu_default_frame $(arg freq)" />
	<node pkg="tf" type="static_transform_publisher" name="subject_heading1" args="0 0.2 0  0 0 0 map opensim_default_frame $(arg freq)" />
	<node pkg="tf" type="static_transform_publisher" name="subject_heading2" args="0 0.3 0  $(arg heading_rad) 0 0 map subject_adds_heading $(arg freq)" />

	<node pkg="osrt_ros" type="wth_node" name="wtfh" >
	
	</node>
</launch>


